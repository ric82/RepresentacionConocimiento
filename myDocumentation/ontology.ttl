@prefix : <http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#> .

<http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations> rdf:type owl:Ontology ;
                                                                                  rdfs:comment "Ontology destinated to Smart Tourism Recommendations" ;
                                                                                  rdfs:label "Smart Tourism Recommendations" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain :Place ;
             rdfs:range :City ;
             rdfs:comment "Relates an entity to the city where it is physically located" ;
             rdfs:label "is located in" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#nearbyAttraction
:nearbyAttraction rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:comment "Connects a hotel to attractions located in proximity" ;
                  rdfs:label "nearby attraction" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#recommendedAttraction
:recommendedAttraction rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdf:type owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:comment "Suggests an attraction likely to appeal to a specific tourist" ;
                       rdfs:label "recommended attraction" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#recommendedHotel
:recommendedHotel rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdf:type owl:AsymmetricProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:comment "Suggests a hotel that matches a tourist's preferences" ;
                  rdfs:label "recommended hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#recommendedRestaurant
:recommendedRestaurant rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdf:type owl:AsymmetricProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:comment "Suggests a restaurant that matches a tourist's dining preferences" ;
                       rdfs:label "recommended restaurant" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#visitsCity
:visitsCity rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Tourist ;
            rdfs:range :City ;
            rdfs:comment "Indicates the current city a tourist is visiting" ;
            rdfs:label "visits city" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#attractionId
:attractionId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Attraction ;
              rdfs:range xsd:integer ;
              rdfs:comment "Unique identifier for an attraction" ;
              rdfs:label "attraction ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#averageCost
:averageCost rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Transportation ;
             rdfs:range xsd:float ;
             rdfs:comment "ypical cost of using a transportation service" ;
             rdfs:label "average cost" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#averagePrice
:averagePrice rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Restaurant ;
              rdfs:range xsd:float ;
              rdfs:comment "Typical cost of a meal at a restaurant" ;
              rdfs:label "average price" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#cityId
:cityId rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :City ;
        rdfs:range xsd:integer ;
        rdfs:comment "Unique identifier for a city" ;
        rdfs:label "city ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#company
:company rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Transportation ;
         rdfs:range xsd:string ;
         rdfs:comment "Organization that operates a transportation service" ;
         rdfs:label "company" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#date
:date rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdf:type owl:FunctionalProperty ;
      rdfs:domain :Event ;
      rdfs:range xsd:string ;
      rdfs:comment "Calendar day when an event takes place" ;
      rdfs:label "date" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#eventId
:eventId rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:integer ;
         rdfs:comment "Unique identifier for an event" ;
         rdfs:label "event ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#hotelId
:hotelId rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :Hotel ;
         rdfs:range xsd:integer ;
         rdfs:comment "Unique identifier for a hotel" ;
         rdfs:label "hotel ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:string ;
          rdfs:comment "Geographic position or region of a city" ;
          rdfs:label "location" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdf:type owl:FunctionalProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range xsd:string ;
      rdfs:comment "Title or designation of an entity" ;
      rdfs:label "name" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#personId
:personId rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Person ;
          rdfs:range xsd:integer ;
          rdfs:comment "Unique identifier for a person" ;
          rdfs:label "person ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#pricePerNight
:pricePerNight rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:float ;
               rdfs:comment "Cost of staying one night at a hotel" ;
               rdfs:label "price per night" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:range xsd:float ;
        rdfs:comment "Numerical evaluation of quality or performance" ;
        rdfs:label "rating" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#restaurantId
:restaurantId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Restaurant ;
              rdfs:range xsd:integer ;
              rdfs:comment "Unique identifier for a restaurant" ;
              rdfs:label "restaurant ID" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#starRating
:starRating rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Hotel ;
            rdfs:range xsd:integer ;
            rdfs:comment "Official classification of hotel quality using stars" ;
            rdfs:label "star rating" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#transportationId
:transportationId rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Transportation ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "Unique identifier for a transportation service" ;
                  rdfs:label "transportation ID" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#AndalusianCuisine
:AndalusianCuisine rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
                   rdfs:comment "Restaurant serving traditional food from the Andalusia region of Spain" ;
                   rdfs:label "Andalusian Cuisine" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf :Place ;
            rdfs:comment "Place or feature that draws visitors due to its inherent interest or appeal" ;
            rdfs:label "Attraction" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :Attraction ;
       rdfs:comment "Coastal area with sand or pebbles suitable for recreation" ;
       rdfs:label "Beach" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BeachHotel
:BeachHotel rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "Hotel located near a beach or with beach access" ;
            rdfs:label "Beach Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BudgetHotel
:BudgetHotel rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Hotel
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :pricePerNight ;
                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                               owl:onDatatype xsd:float ;
                                                                               owl:withRestrictions ( [ xsd:maxExclusive "60.0"^^xsd:float
                                                                                                      ]
                                                                                                    )
                                                                             ]
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Hotel ;
             rdfs:comment "Affordable accommodation with basic amenities" ;
             rdfs:label "Budget Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transportation ;
     rdfs:comment "Road vehicle designed to carry multiple passengers" ;
     rdfs:label "Bus" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BusinessHotel
:BusinessHotel rdf:type owl:Class ;
               rdfs:subClassOf :Hotel ;
               rdfs:comment "Hotel catering to business travelers with appropriate facilities" ;
               rdfs:label "Business Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ;
      rdfs:comment "Urban settlement with defined boundaries and tourism infrastructure" ;
      rdfs:label "City" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#CulturalAttraction
:CulturalAttraction rdf:type owl:Class ;
                    rdfs:subClassOf :Attraction ;
                    rdfs:comment "Site of cultural significance that attracts visitors" ;
                    rdfs:label "Cultural Attraction" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#CulturalEvent
:CulturalEvent rdf:type owl:Class ;
               rdfs:subClassOf :Event ;
               rdfs:comment "Event related to arts, heritage, or cultural expression" ;
               rdfs:label "Cultural Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Place ;
       rdfs:comment "Organized occasion occurring at a specific time and place" ;
       rdfs:label "Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#GastronomicEvent
:GastronomicEvent rdf:type owl:Class ;
                  rdfs:subClassOf :Event ;
                  rdfs:comment "Food or cuisine-centered gathering or festival" ;
                  rdfs:label "Gastronomic Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#HighRatedRestaurant
:HighRatedRestaurant rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Restaurant
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :rating ;
                                                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                       owl:onDatatype xsd:float ;
                                                                                       owl:withRestrictions ( [ xsd:minExclusive "4.0"^^xsd:float
                                                                                                              ]
                                                                                                            )
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Restaurant ;
                     rdfs:comment "Restaurant with exceptional customer reviews and ratings" ;
                     rdfs:label "Highly Rated Restaurant" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :Place ;
       rdfs:comment "Establishment providing accommodation, meals, and other services for travelers" ;
       rdfs:label "Hotel" ;
       owl:sameAs "http://dbpedia.org/ontology/Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#ItalianCuisine
:ItalianCuisine rdf:type owl:Class ;
                rdfs:subClassOf :Restaurant ;
                rdfs:comment "Restaurant specializing in Italian food and cooking traditions" ;
                rdfs:label "Italian Cuisine" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#JapaneseCuisine
:JapaneseCuisine rdf:type owl:Class ;
                 rdfs:subClassOf :Restaurant ;
                 rdfs:comment "Restaurant offering traditional Japanese dishes and cooking styles" ;
                 rdfs:label "Japanese Cuisine" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#LuxuryHotel
:LuxuryHotel rdf:type owl:Class ;
             rdfs:subClassOf :Hotel ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :starRating ;
                               owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:int ;
                                                   owl:withRestrictions ( [ xsd:minInclusive "4"^^xsd:int
                                                                          ]
                                                                          [ xsd:maxInclusive "5"^^xsd:int
                                                                          ]
                                                                        )
                                                 ]
                             ] ;
             rdfs:comment "Upscale hotel offering premium amenities and services" ;
             rdfs:label "Luxury Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#MediterraneanCuisine
:MediterraneanCuisine rdf:type owl:Class ;
                      rdfs:subClassOf :Restaurant ;
                      rdfs:comment "Restaurant serving foods typical of Mediterranean countries" ;
                      rdfs:label "Mediterranean Cuisine" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Metro
:Metro rdf:type owl:Class ;
       rdfs:subClassOf :Transportation ;
       rdfs:comment "Underground urban railway system for passenger transport" ;
       rdfs:label "Metro" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#MexicanCuisine
:MexicanCuisine rdf:type owl:Class ;
                rdfs:subClassOf :Restaurant ;
                rdfs:comment "Restaurant specializing in traditional Mexican dishes and flavors" ;
                rdfs:label "Mexican Cuisine" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Monument
:Monument rdf:type owl:Class ;
          rdfs:subClassOf :Attraction ;
          rdfs:comment "Structure or site preserved for historical significance" ;
          rdfs:label "Monument" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :Attraction ;
        rdfs:comment "Institution that collects, preserves and displays objects of artistic, cultural, or scientific importance" ;
        rdfs:label "Museum" ;
        owl:sameAs "http://dbpedia.org/ontology/Museum" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#MusicEvent
:MusicEvent rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "Concert, festival, or performance focused on musical expression" ;
            rdfs:label "Music Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Park
:Park rdf:type owl:Class ;
      rdfs:subClassOf :Attraction ;
      rdfs:comment "Public recreational area with natural vegetation" ;
      rdfs:label "Park" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Person
:Person rdf:type owl:Class ;
        rdfs:comment "Human individual who can interact with tourism resources" ;
        rdfs:label "Person" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Place
:Place rdf:type owl:Class ;
       rdfs:comment "Physical location with tourism relevance" ;
       rdfs:label "Place" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#PublicBicycle
:PublicBicycle rdf:type owl:Class ;
               rdfs:subClassOf :Transportation ;
               rdfs:comment "Shared bicycle service available for public use" ;
               rdfs:label "Public Bicycle" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf :Place ;
            rdfs:comment "Establishment where meals are prepared and served to customers" ;
            rdfs:label "Restaurant" ;
            owl:sameAs "http://dbpedia.org/ontology/Restaurant" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#RuralHotel
:RuralHotel rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "Hotel located in countryside areas offering natural surroundings" ;
            rdfs:label "Rural Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#SportEvent
:SportEvent rdf:type owl:Class ;
            rdfs:subClassOf :Event ;
            rdfs:comment "Competition, game, or gathering centered on sporting activities" ;
            rdfs:label "Sport Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Taxi
:Taxi rdf:type owl:Class ;
      rdfs:subClassOf :Transportation ;
      rdfs:comment "Vehicle for hire with a driver, used for personal transportation" ;
      rdfs:label "Taxi" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#TechnologyEvent
:TechnologyEvent rdf:type owl:Class ;
                 rdfs:subClassOf :Event ;
                 rdfs:comment "Conference, fair, or exhibition focused on technological innovations" ;
                 rdfs:label "Technology Event" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Tourist
:Tourist rdf:type owl:Class ;
         rdfs:subClassOf :Person ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :visitsCity ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :City
                         ] ;
         rdfs:comment "Person who travels to and stays in places for leisure or business purposes" ;
         rdfs:label "Tourist" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#TouristHotel
:TouristHotel rdf:type owl:Class ;
              rdfs:subClassOf :Hotel ;
              rdfs:comment "Hotel specifically designed for leisure travelers" ;
              rdfs:label "Tourist Hotel" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Tram
:Tram rdf:type owl:Class ;
      rdfs:subClassOf :Transportation ;
      rdfs:comment "Rail vehicle that runs on tracks along public urban streets" ;
      rdfs:label "Tram" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Transportation
:Transportation rdf:type owl:Class ;
                rdfs:subClassOf :Place ;
                rdfs:comment "Means of moving people or goods from one location to another" ;
                rdfs:label "Transportation" .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Barcelona
:Barcelona rdf:type owl:NamedIndividual ,
                    :City ;
           :cityId 4 ;
           :location "41.3874,2.1686" ;
           :name "Barcelona" ;
           rdfs:comment "Major city in Catalonia, Spain, known for its architecture and beaches" ;
           rdfs:label "Barcelona" .


###  http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BarcelonaCathedral
:BarcelonaCathedral rdf:type owl:NamedIndividual ,
                             :Museum ;
