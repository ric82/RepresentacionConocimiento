    <!-- http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#AndalusianCuisine -->


    <owl:Class rdf:about="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#AndalusianCuisine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Restaurant"/>
        <rdfs:comment>Restaurant serving traditional food from the Andalusia region of Spain</rdfs:comment>
        <rdfs:label>Andalusian Cuisine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Attraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Attraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Place"/>
        <rdfs:comment>Place or feature that draws visitors due to its inherent interest or appeal</rdfs:comment>
