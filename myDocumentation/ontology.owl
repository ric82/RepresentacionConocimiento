    <!-- http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BeachHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BeachHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Hotel"/>
        <rdfs:comment>Hotel located near a beach or with beach access</rdfs:comment>
        <rdfs:label>Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BudgetHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#BudgetHotel">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Hotel"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#pricePerNight"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">60.0</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rmart/ontologies/2025/3/SmartTourismRecommendations#Hotel"/>
        <rdfs:comment>Affordable accommodation with basic amenities</rdfs:comment>
        <rdfs:label>Budget Hotel</rdfs:label>
    </owl:Class>
    


